{
  "manifest_version": 2,

  "name": "JS Injector",
  "description": "Allows you to store scripts and inject them in pages",
  "version": "1.0",
  
  "permissions": [
    "activeTab", "tabs", "http://*/*", "https://*/*"
  ],

  "browser_action": {
    "default_icon": "./images/icon.png",
    "default_popup": "popup.html"  
  }
}